// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./books.db"
}

model Book {
  id            Int      @id @default(autoincrement())
  title         String
  author        String
  publishedDate DateTime
}
